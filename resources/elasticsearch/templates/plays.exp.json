                  },
                  "id": {
                     "type": "integer"
                  },
                  "share_stats": {
                     "type": "boolean"
                  },
                  "username": {
                     "type": "string",
                     "index": "not_analyzed"
                  }
               }
            }
         }
      }
   }
}
